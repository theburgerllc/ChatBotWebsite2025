{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/tavus/create-conversation/route.ts": {
      "maxDuration": 15,
      "memory": 512
    },
    "app/api/tavus/webhook/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/tavus/webhook/conversation/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/tavus/webhook/video/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/stripe/checkout/route.ts": {
      "maxDuration": 15,
      "memory": 512
    },
    "app/api/webhooks/stripe/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/contact/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/admin/events/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/admin/webhook-events/route.ts": {
      "maxDuration": 10,
      "memory": 256
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; frame-src 'self' https://tavus.daily.co https://*.daily.co https://*.tavus.io https://checkout.stripe.com; img-src 'self' data: https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com https://js.stripe.com; connect-src 'self' https://tavusapi.com https://*.tavus.io https://*.daily.co wss://*.daily.co https://app.posthog.com https://api.stripe.com; style-src 'self' 'unsafe-inline'; media-src 'self' https://*.daily.co https://*.tavus.io; worker-src 'self' blob:;"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
